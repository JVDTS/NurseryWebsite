{
  "kind": "collectionType",
  "collectionName": "nurseries",
  "info": {
    "singularName": "nursery",
    "pluralName": "nurseries",
    "displayName": "Nursery",
    "description": "Nursery locations and information"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true,
      "unique": true
    },
    "address": {
      "type": "text"
    },
    "phoneNumber": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "description": {
      "type": "richtext"
    },
    "openingHours": {
      "type": "json"
    },
    "heroImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "newsletters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::newsletter.newsletter",
      "mappedBy": "nursery"
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "nursery"
    },
    "galleryImages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::gallery-image.gallery-image",
      "mappedBy": "nursery"
    }
  }
}